<launch>
	<group ns="fmBSP">
		<node pkg="fmBSP" name="COM1" type="serial_node" output="screen" >	 
			<param name="device" value="/dev/ttyUSB0" />	
			<param name="baudrate" value="57600" />
		</node>
	</group>

	<group ns="fmExtractors">
		<node pkg="fmExtractors" name="imu_ypr_extractor" type="imu_ypr" />
	</group>
</launch>

