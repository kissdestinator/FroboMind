<launch>
<group ns="lifecam">
    <node name="lifecam" pkg="uvc_camera" type="camera_node" output="screen">
      <param name="device" type="string" value="/dev/video0"/>
      <param name="camera_info_url" type="string" value="file://$(find rover)/lifecam.yaml"/>
      <param name="width" type="int" value="640"/>
      <param name="height" type="int" value="480"/>
      <param name="fps" type="int" value="20"/>
      <param name="frame" type="string" value="camera"/>
    </node>
  </group>
</launch>
